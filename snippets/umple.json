{
  "Umple Class": {
    "prefix": "class",
    "body": ["class ${1:Name} {", "  ${0}", "}"],
    "description": "Create a class"
  },
  "Umple Trait": {
    "prefix": "trait",
    "body": ["trait ${1:Name} {", "  ${0}", "}"],
    "description": "Create a trait"
  },
  "Umple Association": {
    "prefix": "association",
    "body": [
      "association {",
      "  ${1:0..1} ${2:ClassA} -- ${3:*} ${4:ClassB};",
      "}"
    ],
    "description": "Create an association"
  },
  "Umple State Machine": {
    "prefix": "statemachine",
    "body": [
      "statemachine ${1:name} {",
      "  ${2:StateOne} {",
      "    ${2} -> ${3};",
      "  }",
      "  ${3:StateTwo} {",
      "  }",
      "}"
    ],
    "description": "Create a state machine"
  },

  "Umple Enum": {
    "prefix": "enum",
    "body": ["enum ${1:Name} {", "  ${2:ValueOne}", "}"],
    "description": "Create an enum"
  }
}
