class TimesTable {
  header <<!Print out a <<=times>> times table!>>
  row <<!<<=i>> x <<=times>> = <<=result>>!>>
  cr <<!!>>
  brain;

  emit printHeader(int times)(header, cr);
  emit printRow(int i, int times, int result)(row, cr);
  public emit printFooter()(cr);
  public static emit printAll(int times)(header, row, cr);
}

